    "dnode": "^1.2.2",
    "end-of-stream": "^1.1.0",
    "ensnare": "^1.0.0",
    "eth-bin-to-ops": "^1.0.1",
    "eth-lightwallet": "^2.3.3",
    "eth-query": "^1.0.3",
    "ethereumjs-tx": "^1.0.0",
    "ethereumjs-util": "^4.4.0",
    "ethereumjs-wallet": "^0.6.0",
    "express": "^4.14.0",
    "extension-link-enabler": "^1.0.0",
    "extensionizer": "^1.0.0",
    "gulp-eslint": "^2.0.0",
    "hat": "0.0.3",
